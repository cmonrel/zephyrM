{
  "expo": {
    "name": "ZephyrM",
    "slug": "ZephyrM",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/zephyrLogo.png",
    "scheme": "zephyrmmobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive_zephyrLogo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": ["NFC"],
      "package": "com.cmonrel.ZephyrM"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/zephyrLogo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/zephyrLogo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-secure-store",
        {
          "configureAndroidBackup": true
        }
      ],
      ["react-native-nfc-manager"]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a62e10c8-059f-470b-b87b-9c99a66ce5ee"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/a62e10c8-059f-470b-b87b-9c99a66ce5ee"
    }
  }
}
